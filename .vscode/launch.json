{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Lab Guide Extension",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}",
        "--profile-temp"
      ],
      "outFiles": ["${workspaceFolder}/out/**/*.js"]
    }
  ]
}
